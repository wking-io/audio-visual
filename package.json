    "morgan": "^1.10.0",
    "node-html-parser": "^6.1.13",
    "npm": "^10.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "remix-flat-routes": "^0.6.4",
    "tailwindcss": "^4.0.0-alpha.15",
    "tsx": "^4.9.3",
    "typescript": "^5.4.5",
